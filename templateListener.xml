<?xml version="1.0" encoding="utf-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/eventlistener.xsd">
	<import>
		<templatelistener name="footnoteBlock">
			<templatename>footer</templatename>
			<eventname>contents</eventname>
			<templatecode><![CDATA[{include file='footnotes' application='wcf'}]]></templatecode>
			<environment>user</environment>
		</templatelistener>
		
		<templatelistener name="teraJSFile">
			<environment>user</environment>
			<templatename>headInclude</templatename>
			<eventname>javascriptInclude</eventname>
			<templatecode><![CDATA[{include file='__teraJSFile' application='wcf'}]]></templatecode>
		</templatelistener>
		
		<templatelistener name="xattachButtonInline">
			<templatename>messageFormTabsInline</templatename>
			<eventname>tabMenuContents</eventname>
			<templatecode><![CDATA[{include file='__xAttachButton' application='wcf'}]]></templatecode>
			<environment>user</environment>
		</templatelistener>
		
		<templatelistener name="xattachButtonFull">
			<templatename>messageFormTabs</templatename>
			<eventname>tabMenuContents</eventname>
			<templatecode><![CDATA[{include file='__xAttachButton' application='wcf'}]]></templatecode>
			<environment>user</environment>
		</templatelistener>
		
		<templatelistener name="iconBBCode">
			<templatename>wysiwyg</templatename>
			<eventname>javascriptInit</eventname>
			<templatecode><![CDATA[{include file='__iconEditorPlugin' application='wcf'}]]></templatecode>
			<environment>user</environment>
		</templatelistener>
	</import>
	
	<delete>
		<templatelistener name="xattachButton">
			<templatename>messageFormTabsInline</templatename>
			<eventname>tabMenuContents</eventname>
			<environment>user</environment>
		</templatelistener>
		
		<templatelistener name="xattachButton">
			<templatename>messageFormTabs</templatename>
			<eventname>tabMenuContents</eventname>
			<environment>user</environment>
		</templatelistener>
	</delete>
</data>